{
  "name": "handsfree",
  "version": "7.2.12",
  "description": "A Face Pointer and Pose Estimator for interacting with pages, desktops, robots, and more via gestures",
  "main": "src/handsfree.js",
  "scripts": {
    "start": "vuepress dev docs",
    
    "build": "rm -rf build/lib && rollup --environment INCLUDE_DEPS,BUILD:production --config && vuepress build docs",
    "build:lib": "rm -rf build/lib && rollup --environment INCLUDE_DEPS,BUILD:production --config",
    "build:docs": "vuepress build docs",
    "build:serve": "npm run build:docs && npx http-server build/docs",
    "fa:build": "node node_modules/vuepress-plugin-font-awesome/index.js --dest=docs",

    "serve:cdn": "http-server public/cdn.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/midiblocks/handsfree.git"
  },
  "author": "Oz Ramos <midiblocks@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/midiblocks/handsfree/issues"
  },
  "homepage": "https://github.com/midiblocks/handsfree#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "clippyjs": "0.0.3",
    "rollup": "^2.32.0",
    "rollup-plugin-copy": "^3.3.0",
    "vuepress": "^1.7.1",
    "vuepress-plugin-font-awesome": "^1.90.6"
  },
  "dependencies": {
    "gsap": "^3.5.1",
    "lodash": "^4.17.20"
  }
}
