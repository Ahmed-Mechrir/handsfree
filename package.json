{
  "name": "handsfree",
  "version": "6.1.6",
  "main": "dist/handsfree.js",
  "scripts": {
    "start": "parcel examples/index.pug",
    "bundle": "parcel build src/handsfree.js -d dist",
    "chrome": "parcel build src/handsfree.js -d ../handsfree-chrome/src/handsfree",
    "org": "parcel build src/handsfree.js -d ../handsfree.js.org/node_modules/handsfree/dist"
  },
  "author": "Oz Ramos <heyozramos@gmail.com>",
  "license": "Apache",
  "dependencies": {
    "gsap": "^2.1.3",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "babel-core": "^6.26.3",
    "css-loader": "^3.2.1",
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "pug": "^2.0.4",
    "sass": "^1.23.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 10 Chrome versions"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public"
      }
    ]
  }
}
