{
  "name": "handsfree",
  "version": "6.0.7",
  "main": "dist/handsfree.js",
  "scripts": {
    "start": "parcel examples/index.html",
    "bundle": "parcel build src/handsfree.js -d dist --no-content-hash"
  },
  "author": "Oz Ramos <heyozramos@gmail.com>",
  "license": "Apache",
  "dependencies": {
    "gsap": "^2.1.3",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.3",
    "cssnano": "^4.1.10",
    "parcel-plugin-nuke-dist": "^1.0.1",
    "parcel-plugin-static-files-copy": "^2.2.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "staticFiles": {
    "staticPath": [
      {
        "staticPath": "public/models",
        "staticOutDir": "models"
      }
    ]
  }
}
